{
  "name": "@unional/fixture",
  "description": "Provides fixture to tests",
  "version": "v0.0.0-development",
  "main": "dist-es2015/index.js",
  "typings": "dist-es2015/index.d.ts",
  "files": [
    "dist-es2015"
  ],
  "scripts": {
    "build": "npm run build-es2015",
    "build-es2015": "tsc -p tsconfig.base.json",
    "clean": "rimraf dist dist-es2015",
    "coverage": "jest --coverage",
    "dc": "npm run dependency-check",
    "dependency-check": "dependency-check . --unused --no-dev -i tersify -i @types/diff-match-patch && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json 'src/**/*.ts'",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "tsc && jest",
    "verify": "npm run lint && npm run coverage && npm run build && npm run dependency-check",
    "watch": "jest --watch",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/fixture.git"
  },
  "bugs": {
    "url": "https://github.com/unional/fixture/issues"
  },
  "homepage": "https://github.com/unional/fixture",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFiles": [
      "<rootDir>/scripts/setup-test-env.js"
    ],
    "testRegex": ".*.spec.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "roots": [
      "<rootDir>/src"
    ]
  },
  "devDependencies": {
    "@types/glob": "^5.0.35",
    "@types/jest": "^23.1.1",
    "@types/node": "^10.3.5",
    "assertron": "^5.1.2",
    "aurelia-logging-memory": "^0.3.2",
    "dependency-check": "^3.1.0",
    "eslint": "^5.0.0",
    "eslint-plugin-unional": "^1.0.0",
    "jest": "^23.1.0",
    "path-equal": "^1.1.0",
    "power-assert": "^1.6.0",
    "semantic-release": "^15.6.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-unional": "^0.9.5",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@types/diff-match-patch": "^1.0.32",
    "@unional/logging": "^0.5.3",
    "aurelia-logging-color": "^0.5.15",
    "chalk": "^2.4.1",
    "cp-file": "^6.0.0",
    "diff-match-patch": "^1.0.1",
    "diff-match-patch-line-and-word": "^0.1.2",
    "dir-compare": "^1.4.0",
    "glob": "^7.1.2",
    "minimatch": "^3.0.4",
    "mkdirp": "^0.5.1",
    "pad-left": "^2.1.0",
    "rimraf": "^2.6.2",
    "tersify": "^1.2.6",
    "unpartial": "^0.4.1"
  }
}
