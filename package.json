  "license": "MIT",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "files": [
    "dist-es2015"
  ],
  "main": "dist-es2015/index.js",
  "typings": "dist-es2015/index.d.ts",
  "scripts": {
    "build": "npm run build-es2015",
    "build-es2015": "tsc -p tsconfig.base.json",
    "clean": "rimraf dist dist-es2015",
    "coverage": "jest --coverage",
    "dc": "npm run dependency-check",
    "dependency-check": "dependency-check . --unused --no-dev -i tersify -i @types/diff-match-patch && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "verify": "npm run lint && npm run build && npm run coverage && npm run dependency-check",
    "watch": "jest --watch"
  },
  "dependencies": {
    "@types/diff-match-patch": "^1.0.32",
    "@unional/logging": "^1.0.5",
    "aurelia-logging-color": "^1.0.5",
    "chalk": "^2.4.2",
    "cp-file": "^7.0.0",
    "diff-match-patch": "^1.0.4",
    "diff-match-patch-line-and-word": "^0.1.2",
    "dir-compare": "^2.0.0",
    "glob": "^7.1.4",
    "minimatch": "^3.0.4",
    "mkdirp": "^0.5.1",
    "pad-left": "^2.1.0",
    "rimraf": "^3.0.0",
    "tersify": "^3.0.2",
    "unpartial": "^0.6.3"
  },
  "devDependencies": {
    "@types/cp-file": "^6.1.2",
    "@types/diff": "^4.0.2",
    "@types/glob": "^7.1.1",
    "@types/mkdirp": "^0.5.2",
    "@types/rimraf": "^2.0.2",
    "@unional/devpkg-node": "^1.3.0",
    "assertron": "^7.0.2",
    "aurelia-logging-memory": "^1.0.4",
    "path-equal": "^1.1.1"
  }
}
