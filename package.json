{
  "name": "@unional/fixture",
  "version": "0.0.0-development",
  "description": "Provides fixture to tests",
  "homepage": "https://github.com/unional/fixture",
  "bugs": {
    "url": "https://github.com/unional/fixture/issues"
  },
  "repository": {
    "type": "git",
