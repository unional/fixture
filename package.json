{
  "name": "@unional/fixture",
  "version": "0.0.0-development",
  "description": "Provides fixture to tests",
  "homepage": "https://github.com/unional/fixture",
  "bugs": {
    "url": "https://github.com/unional/fixture/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/fixture.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "type": "module",
  "exports": {
    "import": "./esm/index.js"
  },
  "types": "./esm/index.d.ts",
  "files": [
    "cjs",
    "ts",
    "!ts/**/*.spec.ts"
  ],
  "scripts": {
    "build": "run-p build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "rimraf cjs esm lib fixtures/*/results || true",
    "coverage": "jest --coverage",
    "depcheck": "depcheck",
    "lint": "eslint --ext=ts,js .",
    "prepublishOnly": "yarn bundle",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "test": "jest",
    "verify": "npm-run-all clean -p build depcheck lint coverage",
    "watch": "jest --watch"
  },
  "dependencies": {
    "@types/diff-match-patch": "^1.0.32",
    "chalk": "^4.0.0",
    "cp-file": "^9.1.0",
    "diff-match-patch": "^1.0.4",
    "diff-match-patch-line-and-word": "^0.1.3",
    "dir-compare": "^4.0.0",
    "glob": "^7.2.0",
    "minimatch": "^5.0.0",
    "mkdirp": "^1.0.4",
    "pad-left": "^2.1.0",
    "rimraf": "^3.0.0",
    "standard-log": "^6.2.5",
    "standard-log-color": "^4.0.0",
    "tersify": "^3.8.0",
    "type-plus": "^4.0.0",
    "unpartial": "^0.6.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@types/glob": "^7.2.0",
    "@types/jest": "^28.1.0",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^17.0.7",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "assertron": "^9.0.0",
    "depcheck": "^1.4.3",
    "eslint": "8.17.0",
    "eslint-plugin-harmony": "^7.1.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "path-equal": "^1.1.3",
    "pinst": "^3.0.0",
    "semantic-release": "^19.0.2",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3",
    "uncolor": "^0.4.0"
  }
}
