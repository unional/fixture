{
  "name": "@unional/fixture",
  "description": "Provides fixture to tests",
  "version": "1.2.0",
  "main": "dist-es2015/index.js",
  "typings": "dist-es2015/index.d.ts",
  "files": [
    "dist-es2015"
  ],
  "scripts": {
    "build": "npm run build-es2015",
    "build-es2015": "tsc -p tsconfig.base.json",
    "clean": "rimraf dist dist-es2015",
    "coverage": "jest --coverage",
    "dependency-check": "dependency-check . --unused --no-dev -i tersify && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json 'src/**/*.ts'",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "tsc && jest",
    "verify": "npm run lint && npm run coverage && npm run build && npm run dependency-check",
    "watch": "jest --watch",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/fixture.git"
  },
  "bugs": {
    "url": "https://github.com/unional/fixture/issues"
  },
  "homepage": "https://github.com/unional/fixture",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".*.spec.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "roots": [
      "<rootDir>/src"
    ]
  },
  "devDependencies": {
    "@types/diff": "^3.2.2",
    "@types/glob": "^5.0.35",
    "@types/jest": "^22.1.4",
    "@types/node": "^9.4.6",
    "assertron": "^4.0.0",
    "dependency-check": "^3.0.0",
    "eslint": "^4.18.1",
    "eslint-plugin-unional": "^1.0.0",
    "espower-typescript": "^8.1.3",
    "jest": "^22.4.2",
    "power-assert": "^1.4.4",
    "semantic-release": "^12.4.1",
    "ts-jest": "^22.4.0",
    "tslint": "^5.9.1",
    "tslint-config-unional": "^0.9.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "chalk": "^2.3.1",
    "cp-file": "^5.0.0",
    "diff": "^3.4.0",
    "dir-compare": "^1.4.0",
    "glob": "^7.1.2",
    "minimatch": "^3.0.4",
    "mkdirp": "^0.5.1",
    "pad-left": "^2.1.0",
    "rimraf": "^2.6.2",
    "tersify": "^1.2.2",
    "unpartial": "^0.4.1"
  }
}
